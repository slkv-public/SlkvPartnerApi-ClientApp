@model SwissLife.Slkv.Partner.ClientAppSample.Models.DocumentsModel

@{
    ViewData["Title"] = "Documents";
}

    <h2>Documents</h2>

<div>
    <h4>Documents</h4>
    <hr />
   <ul>
       @foreach (var item in Model.Documents)
       {
           <li>@Html.ActionLink(item.DocumentName, "DownloadDocument", "Request", new { documentId = item.DocumentId }, new { target = "_clear" } )</li>
       }
   </ul>
</div>